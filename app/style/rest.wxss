.clearfix::before,
.clearfix::after {
  content: "";
  display: table;
}

image {
  vertical-align: middle;
}

.clearfix:after {
  clear: both;
}

.main {
  color: #333;
  font: normal 26rpx/1.45 'Microsoft Yahei', Arial, Helvetica, sans-serif;
  box-sizing: border-box;
}

.pd30 {
  padding: 0 30rpx;
}

.pd0 {
  padding: 0 !important;
}

.mg0 {
  margin: 0 !important;
}